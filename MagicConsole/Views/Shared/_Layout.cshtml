<!DOCTYPE html>
<html class="fixed">
<head>

    <!-- Basic -->
    <meta charset="UTF-8">

    <title>Lundgren84 | Admin</title>
    <meta name="keywords" content="Lundgren84 Admin" />
    <meta name="description" content="Lundgren84 Admin">
    <meta name="author" content="Hampus Lundgren">

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <!-- Web Fonts  -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800|Shadows+Into+Light" rel="stylesheet" type="text/css">

    <!-- Vendor CSS -->
    <link rel="stylesheet" href="~/assets/vendor/bootstrap/css/bootstrap.css" />
    <link rel="stylesheet" href="~/assets/vendor/font-awesome/css/font-awesome.css" />
    <link rel="stylesheet" href="~/assets/vendor/magnific-popup/magnific-popup.css" />
    <link rel="stylesheet" href="~/assets/vendor/bootstrap-datepicker/css/datepicker3.css" />

    @* For Code textbox *@
    <link rel="stylesheet" href="~/assets/vendor/codemirror/lib/codemirror.css" />
    <link rel="stylesheet" href="~/assets/vendor/codemirror/theme/monokai.css" />

    @* For Edittable Table *@
    <link rel="stylesheet" href="~/assets/vendor/select2/select2.css" />
    <link rel="stylesheet" href="~/assets/vendor/jquery-datatables-bs3/assets/css/datatables.css" />

    @*Basic form*@
    <link rel="stylesheet" href="~/assets/vendor/bootstrap-fileupload/bootstrap-fileupload.min.css" />

    <!-- Theme CSS -->
    <link rel="stylesheet" href="~/assets/stylesheets/theme.css" />

    <!-- Skin CSS -->
    <link rel="stylesheet" href="~/assets/stylesheets/skins/default.css" />

    <!-- Theme Custom CSS -->
    <link rel="stylesheet" href="~/assets/stylesheets/theme-custom.css">


    <link href="~/static/style/navigation.css" rel="stylesheet" />

    <!-- Head Libs -->
    <script src="~/assets/vendor/modernizr/modernizr.js"></script>

</head>
<body>
    <section class="body">
        @{
            Html.RenderPartial("PlaceHolders/_Header");
        }

        <div class="inner-wrapper">
            @{
                Html.RenderPartial("PlaceHolders/_MainMenu");
            }

            <section role="main" class="content-body">
                @{
                    Html.RenderPartial("PlaceHolders/_BreadCrumbs");
                }
                <div class="row">
                    <div id="mainContentArea" class="col-xs-12">
                        @RenderBody()
                    </div>
                </div>
            </section>
        </div>
        @{
            Html.RenderPartial("PlaceHolders/_SidebarRight");
        }
    </section>

    @Scripts.Render("~/bundles/vendor")
    @Scripts.Render("~/bundles/codeeditor")
    @Scripts.Render("~/bundles/theme")

    <!-- Examples -->
    <script src="~/assets/javascripts/dashboard/examples.dashboard.js"></script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/main")
    @RenderSection("scripts", required: false)
</body>
</html>